<%= form_for :report, url: update_report_path, html: { class: 'form-inline' } do |f| %>
  <div class="form-group">
    <%= f.label :start_date %>
    <%= f.text_field :start_date, class: 'datepicker', value: @start_date.strftime('%m/%d/%Y') %>
  </div>
  <div class="form-group">
    <%= f.label :end_date %>
    <%= f.text_field :end_date, class: 'datepicker', value: @end_date.strftime('%m/%d/%Y') %>
  </div>
  <%= submit_tag 'Change Date Range', class: 'btn btn-primary submit-button' %>
<% end %> 
